		usbpd_irq: usbpd-irq {
			samsung,pins = "gpa1-3";
			samsung,pin-function = <0x0f>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

	};

	i2c@13850000 {
		i2c-gpio,delay-us = <5>;
		status = "okay";
		usbpd-5713@33 {
			compatible = "sm5713-usbpd,i2c";
			reg = <0x33>;
			pinctrl-names = "default";
			vconn-en;
			pinctrl-0 = <&usbpd_irq>;			
			usbpd,usbpd_int = <&gpa1 3 0>;			
		};
	};


